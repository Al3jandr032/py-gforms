# Google Forms API Configuration

# API Key Authentication (legacy method)
GOOGLE_API_KEY=your_google_api_key_here

# Service Account Authentication (recommended)
# Path to service account JSON file
GOOGLE_SERVICE_ACCOUNT_PATH=/path/to/service-account.json
# OR service account JSON as string (for containerized environments)
# GOOGLE_SERVICE_ACCOUNT_JSON={"type":"service_account",...}
# Force use of service account authentication
USE_SERVICE_ACCOUNT=false

# OAuth Authentication (for user-based access)
GOOGLE_CLIENT_SECRETS_PATH=/path/to/client_secrets.json
GOOGLE_TOKEN_PATH=token.json

# Application Configuration
DEBUG=false
LOG_LEVEL=INFO
